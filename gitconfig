	pretty = log --pretty=format:'%C(yellow)%h %C(green)%ad %C(cyan)[%an] %Creset%s' --date=format:'%F %T' --decorate --graph
	pop = stash pop
	rename = branch -m
	forcepull = !~/forcepull.sh
	show-files = show --name-status
